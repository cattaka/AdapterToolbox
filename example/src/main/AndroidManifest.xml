<?xml version="1.0" encoding="utf-8"?>
<manifest package="net.cattaka.android.adaptertoolbox.example"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".CodeLabelExampleActivity"/>
        <activity android:name=".SimpleStringExampleActivity"/>
        <activity android:name=".RecyclerViewHeaderExampleActivity"/>
        <activity android:name=".MultiAdapterExampleActivity"/>
        <activity android:name=".ComplexStringExampleActivity"/>
        <activity android:name=".ScrambleAdapterExampleActivity"/>
        <activity android:name=".ManipulableListExampleActivity"/>
        <activity android:name=".NestedScrambleAdapterExampleActivity"/>
        <activity android:name=".TreeItemAdapterExampleActivity"/>
        <activity android:name=".MultiChoosableTreeItemAdapterExampleActivity"/>
        <activity android:name=".SingleChoosableTreeItemAdapterExampleActivity"/>
        <activity android:name=".ClassicScrambleAdapterExampleActivity"/>
        <activity android:name=".SpinnerScrambleAdapterExampleActivity"/>
        <activity android:name=".SpinnerTreeItemAdapterExampleActivity"/>
    </application>

</manifest>